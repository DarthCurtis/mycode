variable "container_name" {
  description = "name fo the container"
  type        = string
  default     = "Alta3ResearchWebService"
}

variable "internal_port" {
  description = "internal port number"
  type        = number
  default     = 9876
}

variable "external_port" {
  description = "external port number"
  type        = number
  default     = 5432
}
